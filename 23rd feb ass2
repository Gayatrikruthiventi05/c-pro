//write program of all the odd numbers except the third one on the range//
#include <stdio.h>
int main()
{
    int n,i;
printf("Print odd numbers till: ");
scanf("%d",&n);
printf("All odd numbers from n to %d are:\n");
for(i=10;i<=20;i++)
    {
        if(i%2==1 && i!=15)
        {
            printf("%d\n", i);
        }
    }
 return 0;
}
